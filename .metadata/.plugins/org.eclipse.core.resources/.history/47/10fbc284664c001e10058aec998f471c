<script>
	
	let snack = {
		name:"고래밥",
		price:3800
	};
	console.log(snack);
	
	let score={
		name:"홍길동",
		kor:90,
		eng:90,
		math:80,
		tot:0,
		calcTot:function(){
			this.tot = this.kor+this.eng+this.math;
		},
		hi:function(){ // hi라는 이름의 function
			alert("score 객체 hi");
		}
	};
	score.hi();
	console.log(score.tot); //tot출력
	score.calcTot();
	console.log(score.calcTot);
</script>
