<script>
	
	function send(){
		$.ajax({
			type:"get",
			dataType:"text",
			url:"/ajaxprj0906복습/food0",
			success:function(data){
				$("#result").html(data);
				console.log(data);
				
			},
			error: function(err){
				console.log(err);
			}
		});
	}
</script>
